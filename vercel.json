{
  "version": 2,
  "name": "246-n-3rd-property-portal",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "app/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}